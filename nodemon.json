{
  "ignore": [
    "./**/*.spec.ts",
    "./**/*.test.ts",
    ".git",
    "node_modules"
  ],
  "watch": [
      "src/**/*.ts",
      "src/**/*.d.ts"
  ],
  "ext": "ts",
  "exec": " TS_NODE_FILES=true node --inspect=5858 -r ts-node/register -r tsconfig-paths/register  ./src/index.ts --env=development"
}
